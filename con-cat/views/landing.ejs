<% include partials/header %>

<!--The content below is only a placeholder and can be replaced.-->
<div style="text-align:center">
  <h1>
    Welcome to the {{title}}!
  </h1>
  <img width="300" src="data:image/svg+xml;base64,">
</div>
<div>
  <label>Test Form: </label>
  <!-- Form to submit new data -->
  <form action="/" method="POST">
      <input [(ngModel)]="testfile.name" placeholder="Placeholder" name="testdata">
      <button>Submit</button>
  </form>
  <p>
      <a href="/new">Click here to submit data. Or use form above</a>
  </p>
</div>
<h1>Upload your file here:</h1>
<h1>Click here to navigate uploaded files.</h1>
<h2>My Files</h2>
  <ul class="files">
    <!-- each file goes here -->
    <% testData.forEach(function(data){ %>
        <li><%= data.name %></li>
    <% }) %>
    <li *ngFor="let file of files">
      <span class="badge">{{file.id}}</span>
      {{file.name}}
    </li>
  </ul>

